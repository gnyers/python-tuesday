network:
  version: 2
  ethernets:
    eth0:
      addresses: [ ]
      dhcp4: true
      dhcp6: true
      nameservers: {}
      optional: true

output:
  init:
    output: "> /var/log/cloud-init.out"
    error: "> /var/log/cloud-init.err"
  config: "tee -a /var/log/cloud-config.log"
  final:
    - ">> /var/log/cloud-final.out"
    - "/var/log/cloud-final.err"

rsyslog:
    remotes:
        log_serv: "*.* @{{ loghost_url }}"

package_upgrade: true
packages:
  - pwgen
  - telnet

phone_home:
  url: {{ phone_home_url }}
  post: [ ]
  tries: 5

